{
  "name": "skaiscraper",
  "private": true,
  "version": "0.0.1",
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=22 <23",
    "pnpm": ">=10.0.0"
  },
  "type": "module",
  "config": {
    "puppeteer_skip_chromium_download": "true"
  },
  "scripts": {
    "postinstall": "PUPPETEER_SKIP_DOWNLOAD=true PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD=1 prisma generate",
    "dev": "next dev",
    "dev:agents": "bash scripts/dev-agents.sh",
    "agent:worker": "node --import tsx scripts/agentWorker.ts",
    "worker": "node --import tsx src/worker/index.ts",
    "worker:dev": "NODE_ENV=development node --import tsx --watch src/worker/index.ts",
    "vercel-build": "node scripts/env-preflight.js && node scripts/build-ci.js",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' BUILD_PHASE=1 next build",
    "regression:check": "node scripts/regression-guards.cjs",
    "prisma:generate": "prisma generate",
    "prisma:validate": "prisma validate",
    "guard:prisma": "bash scripts/guard-no-extra-prisma.sh",
    "validate:schema": "tsx tools/schemaValidator.ts",
    "validate:schema:fix": "tsx tools/schemaValidator.ts --fix",
    "demo:seed": "tsx scripts/demo-seed.ts",
    "demo:qa": "node scripts/demo-qa.cjs",
    "verify:template-assets": "node scripts/verify-template-assets.mjs",
    "start": "next start",
    "start:web": "next start",
    "lint": "npm run lint:core",
    "lint:core": "eslint ./src/app ./src/components ./src/lib --max-warnings=0",
    "clean": "rimraf .next .turbo dist && rimraf node_modules/.cache || true",
    "test": "playwright test --project=smoke",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:all": "playwright test",
    "test:smoke": "playwright test --project=smoke",
    "test:e2e": "playwright test --project=e2e",
    "test:e2e:ui": "playwright test --ui",
    "test:api": "playwright test --project=smoke tests/api/",
    "test:auth": "playwright test --project=smoke tests/api/auth-matrix.spec.ts",
    "test:isolation": "playwright test --project=smoke tests/api/org-isolation.spec.ts",
    "test:sprint26": "playwright test --project=smoke tests/api/sprint26-critical-flows.spec.ts",
    "coverage:routes": "node scripts/route-coverage-map.cjs",
    "coverage:routes:json": "node scripts/route-coverage-map.cjs --json",
    "coverage:unprotected": "node scripts/route-coverage-map.cjs --unprotected",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "seed:validate": "node scripts/validate-seeds.mjs",
    "db:diff": "prisma migrate diff --from-schema-datamodel prisma/schema.prisma --to-schema-datasource prisma/schema.prisma --script > db/migrations/drift_$(date +%Y%m%d_%H%M%S).sql",
    "db:check": "node scripts/db-check.mjs",
    "db:migrate": "pnpm prisma migrate deploy",
    "db:generate": "pnpm prisma generate",
    "db:repair": "node scripts/prisma-repair.mjs",
    "db:deploy": "pnpm run db:check && pnpm run db:migrate && pnpm run db:generate",
    "cli:bootstrap-org": "tsx cli/bootstrap-org.ts",
    "cli:force-tokens": "tsx cli/force-tokens.ts",
    "verify-routes": "node scripts/verify-routes.mjs",
    "verify:left-nav": "tsx scripts/verify-left-nav-pages.ts",
    "validate:routes": "tsx tools/validateRoutes.ts",
    "agents:dry-run": "tsx scripts/agents/dry-run.ts",
    "test-ai-endpoints": "node --import tsx scripts/test-ai-endpoints.ts",
    "verify": "pnpm verify-routes && pnpm test-ai-endpoints",
    "smoke:prod": "node scripts/postdeploy-smoke.mjs",
    "smoke:local": "node scripts/postdeploy-smoke.mjs http://localhost:3000",
    "smoke:db": "node scripts/smoke/db.cjs",
    "smoke:ready": "node scripts/smoke/ready.cjs",
    "smoke:runtime": "node scripts/smoke-runtime.mjs",
    "release:verify": "node scripts/release-verify.mjs",
    "release:smoke": "node scripts/release-smoke.mjs",
    "release:go": "node scripts/release-go-no-go.mjs",
    "db:smoke": "tsx scripts/db-smoke.ts",
    "test:build-guard": "tsx scripts/test-build-guard.ts",
    "seed:ai:test": "tsx scripts/seed_ai_test_data.ts",
    "seed:test-claim": "tsx scripts/seed-test-claim.ts",
    "seed:emergency-demo": "tsx prisma/seed-emergency-demo.ts",
    "seed:demo": "tsx prisma/seed-demo.ts",
    "seed:minimal-demo": "tsx prisma/seed-minimal-demo.ts",
    "seed:vendors": "tsx scripts/seed-vendors.ts",
    "format": "prettier --write .",
    "check": "pnpm format && pnpm lint",
    "verify:env": "tsx scripts/verify-env.ts",
    "ai:diagnostic": "tsx scripts/ai-tools-diagnostic.ts",
    "report:generate": "tsx scripts/generate-report.ts",
    "validate:layout": "tsx scripts/validate-layout-wrapping.ts",
    "lint:tokens": "tsx scripts/audit-tailwind-tokens.ts",
    "typecheck": "tsc -p tsconfig.typecheck.json",
    "audit:routes": "tsx scripts/prod-audit/audit_routes.ts",
    "audit:env": "tsx scripts/prod-audit/audit_env_usage.ts",
    "audit:storage": "tsx scripts/prod-audit/audit_storage.ts",
    "audit:all": "pnpm audit:routes && pnpm audit:env && pnpm audit:storage",
    "templates:generate-previews": "tsx scripts/generate-missing-template-previews.ts",
    "gen:premium-previews": "tsx scripts/generate-premium-previews.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
